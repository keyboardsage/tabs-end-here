{
    "manifest_version": 3,
    "name": "Tabs End Here",
    "version": "0.1",
    "description": "Prevent opening new tabs unless one is closed first.",
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "tabs"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icon.png"
    }
}
